{
  "name": "dockyard-electron",
  "version": "1.0.0",
  "description": "A flexible, open-source multi-app workspace built for power users.",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "npm run build:main && concurrently \"npm run watch:main\" \"npm run dev:renderer\" \"npm run start:electron\"",
    "dev:renderer": "vite",
    "build:main": "tsc -p tsconfig.main.json",
    "watch:main": "tsc -p tsconfig.main.json --watch",
    "build:renderer": "vite build",
    "build": "npm run build:main && npm run build:renderer",
    "start:electron": "wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "start": "npm run build && electron .",
    "package": "npm run build && electron-forge package",
    "make": "npm run build && electron-forge make",
    "publish": "npm run build && electron-forge publish",
    "format": "prettier --write 'src/**/*.{js,ts,jsx,tsx,css,scss,html,json,md}'",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "popular-apps:organize": "node scripts/organize-popular-apps.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MayR-Labs/dockyard-electron.git"
  },
  "keywords": [
    "workspace",
    "open-source"
  ],
  "author": "Aghogho Meyoron (https://mayrlabs.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MayR-Labs/dockyard-electron/issues"
  },
  "homepage": "https://github.com/MayR-Labs/dockyard-electron#readme",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/publisher-github": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^11.0.2",
    "framer-motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zustand": "^5.0.8"
  }
}
